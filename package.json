{
  "name": "n8n-nodes-html-to-pdf",
  "version": "1.0.0",
  "description": "n8n node for converting HTML to PDF using Playwright (optimized for threat reports)",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "html",
    "pdf",
    "converter",
    "playwright",
    "threat-report"
  ],
  "license": "MIT",
  "homepage": "https://github.com/cce15/n8n-nodes-html-to-pdf#readme",
  "author": {
    "name": "Abo Alhasan",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cce15/n8n-nodes-html-to-pdf.git"
  },
  "bugs": {
    "url": "https://github.com/cce15/n8n-nodes-html-to-pdf/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/HtmlToPdf/HtmlToPdf.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.40.0",
    "eslint-plugin-n8n-nodes-base": "^1.12.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "playwright": "^1.40.0"
  }
}
